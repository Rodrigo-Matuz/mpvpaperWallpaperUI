{
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "es6": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "arrow-spacing": ["warn", { "before": true, "after": true }],
        "no-console": "off",
        "prefer-const": "error",
        "quotes": ["error", "double"]
    }
}
